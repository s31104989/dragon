{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/dragon",
  "version": "0.1.0",
  "title": "Dragon",
  "category": "widgets",
  "icon": "smiley",
  "description": "The WordPress plugin that colors the difference between two texts.",
  "supports": {
    "html": false
  },
  "textdomain": "dragon",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "oldCode": {
      "type": "string",
      "source": "text",
      "selector": ".oldCode",
      "default": ""
    },
    "newCode": {
      "type": "string",
      "source": "text",
      "selector": ".newCode",
      "default": ""
    }
  }
}